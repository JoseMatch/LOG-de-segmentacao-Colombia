Select INICIAL.RUNDATE, INICIAL.CD_PAIS, INICIAL.DC_NOME_CONSULTORA, INICIAL.DC_NO_PRIMEIRO, INICIAL.DC_SEXO,
INICIAL.DT_NASCIMENTO_PESSOA, INICIAL.IDADE, INICIAL.DC_CELULAR, INICIAL.CD_NVL_PLAN_CREC, INICIAL.DC_NVL_PLAN_CREC, 
INICIAL.DC_PAPEL, INICIAL.CD_PAPEL, INICIAL.CD_ESTRUT_COMERCIAL, INICIAL.DC_ESTRUT_COMERCIAL, 
INICIAL.CD_ESTRUT_COMERCIAL_SUP, INICIAL.DC_ESTRUT_COMERCIAL_SUP, INICIAL.CD_STATUS, INICIAL.DC_STATUS,
INICIAL.CD_CICLO_PAI, INICIAL.CD_CICLO_ATUAL, INICIAL.DC_CICLO_ATUAL, INICIAL.CD_SUB_CICLO, INICIAL.D_CICLO, 
INICIAL.D_TERMINO, INICIAL.DT_INICIO, INICIAL.DT_FIM, INICIAL.DT_INICIO_FORM, INICIAL.DT_TERMINO_FORM, 
INICIAL.QT_CICLOS_CASA, INICIAL.PED_VALOR_TOTAL, INICIAL.PED_VALOR_TOTAL_TXT, INICIAL.PED_PONTOS_TOTAL, 
INICIAL.PED_PONTOS_TOTAL_TXT, INICIAL.PED_QTD_PEDIDO, INICIAL.PED_QTD_PEDIDO_TXT, INICIAL.COM_PEDIDO, 
INICIAL.CD_NIVEL_INATIVIDADE, INICIAL.INICIOS, INICIAL.GC_UNIVERSAL, INICIAL.ELEG_SMS, INICIAL.ELEG_EMAIL,
INICIAL.ELEG_PUSH, INICIAL.ELEG_CARD, INICIAL.MOROSA, INICIAL.TEMPO_CASA, INICIAL.DC_EMAIL, INICIAL.ID_CONTACT,
INICIAL.CD_CONSULTORA,INICIAL.CD_CICLO_ATUAL AS CICLO,'' AVON_PEDIDOS, inicial.FL_MIGRADA_AVON,

/*ID_Campaign: 
- Si ya hay audiencia con un Campaign_id lo uso. 
- Sino tomo el más nuevo de la tabla Campaigns para este país. (Creado en el paso anterior)*/
C1.CAMPAIGN_ID AS [campaignid],

/*Selección de perfil*/
case 
    when aux.perfil is not null then aux.perfil
    when perfil1.cd_pessoa is not null then 1
    when perfil2.CD_CONSULTORA is not null then 2
    when perfil3.CD_CONSULTORA is not null then 3
    else 0
end as Perfil

/*Priorización de canales*/
,CASE WHEN inicial.ELEG_EMAIL = 1 AND INICIAL.DC_EMAIL IS NOT NULL THEN 1 ELSE 0 END AS EMAIL
,CASE WHEN inicial.ELEG_PUSH = 1 THEN 1 ELSE 0 END AS PUSH
,CASE WHEN inicial.ELEG_SMS = 1 AND inicial.ELEG_EMAIL = 0 AND inicial.ELEG_PUSH = 0 THEN 1 ELSE 0 END AS SMS

FROM 
    [CHI_202508_AUDIENCIA1] inicial
    Left join 
    [CHI_202508_AUDIENCIA_P1] perfil1
    on inicial.cd_consultora = perfil1.cd_pessoa 
    left join
    [CHI_202508_AUDIENCIA1_P2] perfil2
    on inicial.CD_CONSULTORA=perfil2.CD_CONSULTORA
    Left join 
    [CHI_202508_AUDIENCIA1_P3] perfil3
    on inicial.CD_CONSULTORA=perfil3.CD_CONSULTORA

    
    LEFT JOIN 
        (SELECT TOP 1 CAMPAIGN_ID FROM [Campaigns] C1 WHERE PAIS='CH' AND DESCRIPTION LIKE 'Jornada%202508' ORDER BY SUFFIX DESC) C1 ON 1=1
LEFT JOIN  [CHI_202508_AUDIENCIA3_AUX] AUX ON AUX.ID_CONTACT = INICIAL.ID_CONTACT
